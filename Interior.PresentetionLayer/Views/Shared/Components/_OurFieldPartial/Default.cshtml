@model List<Interior.DomainLayer.Concrete.OurField>
<section class="grids-1 py-5">
    <div class="grids py-lg-5 py-md-4">
        <div class="container">
            <h3 class="title-big">Ne Yapıyoruz</h3>
            <div class="row text-center grids-gap">
                @foreach (var item in Model)
                {
                    <div class="col-lg-3 col-md-4 col-sm-6">
                        <div class="column">
                            <a href="#single"><img src="@item.OurFieldImage" alt="" class="img-responsive" /></a>
                            <h4><a href="#single">@item.OurFieldName</a></h4>
                        </div>
                    </div>

                }
              
            </div>
        </div>
    </div>
</section>