@model List<Interior.DomainLayer.Concrete.Team>
<section class="w3l-team py-5" id="w3l-team">
    <div class="container py-lg-4 py-sm-3">
        <h3 class="title-big mb-md-5 mb-4">Takımımız</h3>
        <div class="row text-center">
            @foreach (var item in Model)
            {
                <div class="col-md-3 col-6">
                    <div class="team-grids text-center">
                        <img src="@item.TeamSurname" class="img-fluid" alt="">
                        <div class="team-info">
                            <div class="caption">
                                <div class="social-icons-section text-center">
                                    <a class="fac" href="#url">
                                        <i class="fa fa-facebook"></i>
                                    </a>
                                    <a class="twitter" href="#url">
                                        <i class="fa fa-twitter"></i>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <h4>@item.TeamName</h4>
                    <h6>@item.TeamDepartment</h6>
                </div>
            }
            
            
        </div>
    </div>
</section>